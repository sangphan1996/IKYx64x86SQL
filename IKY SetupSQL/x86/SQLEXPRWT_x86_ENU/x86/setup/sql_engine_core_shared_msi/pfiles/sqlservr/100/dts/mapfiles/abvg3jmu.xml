			<dtm:DataTypeName>Double</dtm:DataTypeName>
		</dtm:SourceDataType>
		<dtm:DestinationDataType>
			<dtm:SimpleType>
				<dtm:DataTypeName>float</dtm:DataTypeName>
			</dtm:SimpleType>
		</dtm:DestinationDataType>
	</dtm:DataTypeMapping>	
	
	<!-- Currency -->
	<dtm:DataTypeMapping >
		<dtm:SourceDataType>
			<dtm:DataTypeName>Currency</dtm:DataTypeName>
		</dtm:SourceDataType>
		<dtm:DestinationDataType>
			<dtm:SimpleType>
				<dtm:DataTypeName>money</dtm:DataTypeName>
			</dtm:SimpleType>
		</dtm:DestinationDataType>
	</dtm:DataTypeMapping>	
	

	<!-- Bit -->
	<dtm:DataTypeMapping >
		<dtm:SourceDataType>
			<dtm:DataTypeName>Bit</dtm:DataTypeName>
		</dtm:SourceDataType>
		<dtm:DestinationDataType>
			<dtm:SimpleType>
				<dtm:DataTypeName>bit</dtm:DataTypeName>
			</dtm:SimpleType>
		</dtm:DestinationDataType>
	</dtm:DataTypeMapping>	
	
	<!-- Byte -->
	<dtm:DataTypeMapping>
		<dtm:SourceDataType>
			<dtm:DataTypeName>Byte</dtm:DataTypeName>
		</dtm:SourceDataType>
		<dtm:DestinationDataType>
			<dtm:SimpleType>
				<dtm:DataTypeName>tinyint</dtm:DataTypeName>
			</dtm:SimpleType>
		</dtm:DestinationDataType>
	</dtm:DataTypeMapping>	
	
	<!-- GUID -->	
	<dtm:DataTypeMapping>
		<dtm:SourceDataType>
			<dtm:DataTypeName>GUID</dtm:DataTypeName>
		</dtm:SourceDataType>
		<dtm:DestinationDataType>
			<dtm:SimpleType>
				<dtm:DataTypeName>uniqueidentifier</dtm:DataTypeName>
			</dtm:SimpleType>
		</dtm:DestinationDataType>
	</dtm:DataTypeMapping>	
	
	<!-- BigBinary -->
	<dtm:DataTypeMapping>
		<dtm:SourceDataType>
			<dtm:DataTypeName>BigBinary</dtm:DataTypeName>
		</dtm:SourceDataType>
		<dtm:DestinationDataType>
			<dtm:CharacterStringType>
				<dtm:DataTypeName>binary</dtm:DataTypeName>
				<dtm:UseSourceLength/>
			</dtm:CharacterStringType>
		</dtm:DestinationDataType>
	</dtm:DataTypeMapping>	
	
	<!-- LongBinary -->
	<dtm:DataTypeMapping >
		<dtm:SourceDataType>
			<dtm:DataTypeName>LongBinary</dtm:DataTypeName>
		</dtm:SourceDataType>
		<dtm:DestinationDataType>
			<dtm:SimpleType>
				<dtm:DataTypeName>image</dtm:DataTypeName>
			</dtm:SimpleType>
		</dtm:DestinationDataType>
	</dtm:DataTypeMapping>	

	<!-- VarBinary -->
	<dtm:DataTypeMapping >
		<dtm:SourceDataType>
			<dtm:DataTypeName>VarBinary</dtm:DataTypeName>
		</dtm:SourceDataType>
		<dtm:DestinationDataType>
			<dtm:CharacterStringType>
				<dtm:DataTypeName>varbinary</dtm:DataTypeName>
