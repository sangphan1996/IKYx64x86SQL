		<dtm:SourceDataType>
			<dtm:DataTypeName>LongText</dtm:DataTypeName>
		</dtm:SourceDataType>
		<dtm:DestinationDataType>
			<dtm:SimpleType>
				<dtm:DataTypeName>ntext</dtm:DataTypeName>
			</dtm:SimpleType>
		</dtm:DestinationDataType>
	</dtm:DataTypeMapping>	

	<!-- VarChar -->
	<dtm:DataTypeMapping >
		<dtm:SourceDataType>
			<dtm:DataTypeName>VarChar</dtm:DataTypeName>
		</dtm:SourceDataType>
		<dtm:DestinationDataType>
			<dtm:CharacterStringType>
				<dtm:DataTypeName>nvarchar</dtm:DataTypeName>
				<dtm:UseSourceLength/>
			</dtm:CharacterStringType>
		</dtm:DestinationDataType>
	</dtm:DataTypeMapping>	

	<!-- Decimal -->
	<dtm:DataTypeMapping >
		<dtm:SourceDataType>
			<dtm:DataTypeName>Decimal</dtm:DataTypeName>
		</dtm:SourceDataType>
		<dtm:DestinationDataType>
			<dtm:NumericType>
				<dtm:DataTypeName>decimal</dtm:DataTypeName>
				<dtm:UseSourcePrecision/>
				<dtm:UseSourceScale/>
			</dtm:NumericType>
		</dtm:DestinationDataType>
	</dtm:DataTypeMapping>	

	<!-- DateTime -->
	<dtm:DataTypeMapping >
		<dtm:SourceDataType>
			<dtm:DataTypeName>DateTime</dtm:DataTypeName>
		</dtm:SourceDataType>
		<dtm:DestinationDataType>
			<dtm:SimpleType>
				<dtm:DataTypeName>datetime</dtm:DataTypeName>
			</dtm:SimpleType>
		</dtm:DestinationDataType>
	</dtm:DataTypeMapping>	
	
</dtm:DataTypeMappings>
