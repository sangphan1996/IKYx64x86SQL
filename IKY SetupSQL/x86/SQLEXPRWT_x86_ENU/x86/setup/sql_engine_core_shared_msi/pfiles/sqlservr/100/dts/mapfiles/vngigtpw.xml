		<dtm:SourceDataType>
			<dtm:DataTypeName>TIME</dtm:DataTypeName>
		</dtm:SourceDataType>
		<dtm:DestinationDataType>
			<dtm:SimpleType>
				<dtm:DataTypeName>DATETIME</dtm:DataTypeName>
			</dtm:SimpleType>
		</dtm:DestinationDataType>
	</dtm:DataTypeMapping>	

